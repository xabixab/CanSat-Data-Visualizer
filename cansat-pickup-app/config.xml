<?xml version='1.0' encoding='utf-8'?>
<widget id="com.iturramasat.pickup" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>IS. Pick-Up</name>
    <description>
        Displays CanSat GPS Coords in a map.
    </description>
    <author email="xabier@xabixab.com" href="http://xabixab.com">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="Fullscreen" value="true" />
        <icon density="mdpi" src="assets/mdpi.png" />
        <icon density="hdpi" src="assets/hdpi.png" />
        <icon density="xhdpi" src="assets/xhdpi.png" />
        <icon density="xxhdpi" src="assets/xxhdpi.png" />
        <icon density="xxxhdpi" src="assets/xxxhdpi.png" />
    </platform>
    <plugin name="cordova-plugin-geolocation" spec="~2.4.2" />
    <plugin name="cordova-plugin-device" spec="~1.1.5" />
    <engine name="android" spec="~6.1.2" />
</widget>
